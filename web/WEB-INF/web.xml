<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>R1</servlet-name>
        <servlet-class>returnPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>R1</servlet-name>
        <url-pattern>/getPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>R2</servlet-name>
        <servlet-class>Overview</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>R2</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>R3</servlet-name>
        <servlet-class>Overview</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>R3</servlet-name>
        <url-pattern>/lookData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>R4</servlet-name>
        <servlet-class>sqlProcess</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>R4</servlet-name>
        <url-pattern>/SQLquery</url-pattern>
    </servlet-mapping>
</web-app>